{
    "name": "typescript-node-starter",
    "version": "0.0.1",
    "description": "Typescript node package starter project",
    "keywords": [
        "typescript",
        "node",
        "starter",
        "seed"
    ],
    "license": "MIT",
    "author": "squaremarco",
    "main": "dist/src/index.js",
    "scripts": {
        "build": "run-p --print-label build:src build:bin build:dts",
        "build:bin": "babel bin --out-dir dist/bin --extensions .js,.ts --source-maps",
        "build:bin:watch": "npm run build:bin -- --watch \"bin/**/*\" --verbose",
        "build:dev": "cross-env NODE_ENV=development npm run build:src:watch",
        "build:dts": "tsc --emitDeclarationOnly",
        "build:dts:watch": "npm run build:dts -- --watch --preserveWatchOutput",
        "build:src": "babel src --out-dir dist/src --extensions .js,.ts --source-maps",
        "build:src:watch": "npm run build:src -- --watch \"src/**/*\" --verbose",
        "build:watch": "onchange \"src/**/*\" --initial --kill --delay 1000  -- npm run build",
        "display-browser-support": "browserslist --stats",
        "format": "run-s format:imports format:prettier format:eslint format:package-json",
        "format:eslint": "eslint --cache --ext .js,.ts --fix ./",
        "format:imports": "import-sort --write \"**/*.{js,ts}\"",
        "format:package-json": "sort-package-json",
        "format:prettier": "prettier --write \"**/*.{js,json,md,ts,yaml,yml}\" \".editorconfig\" \"LICENSE.md\"",
        "lint": "run-p lint:eslint lint:markdown",
        "lint:eslint": "eslint --cache --ext .js,.ts, --format=pretty ./",
        "lint:markdown": "markdownlint --ignore coverage --ignore dist --ignore node_modules \"**/*.md\" \".**/**/*.md\"",
        "lint:watch": "onchange \"src/**/*\" --initial --kill --delay 1000 -- npm run lint",
        "prepublish": "rimraf dist && npm run build",
        "start": "babel-node --extensions .js,.ts -r ./node_modules/dotenv/config src/index.ts",
        "start:dev": "cross-env NODE_ENV=development npm start",
        "start:watch": "nodemon --ext js,ts --delay 1 --exec npm start",
        "test": "jest --passWithNoTests",
        "test:coverage": "npm run test -- --coverage",
        "test:watch": "npm run test -- --watch",
        "typecheck": "tsc --noEmit",
        "typecheck:watch": "npm run typecheck -- --watch",
        "validate": "run-p --print-label lint typecheck test build"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -p false"
        }
    },
    "dependencies": {
        "dotenv": "^8.2.0",
        "express": "^4.17.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.9.6",
        "@babel/node": "^7.8.7",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
        "@babel/preset-env": "^7.9.6",
        "@babel/preset-typescript": "^7.9.0",
        "@types/express": "^4.17.6",
        "@types/jest": "^25.2.1",
        "@types/node": "^13.13.5",
        "@typescript-eslint/eslint-plugin": "^2.31.0",
        "@typescript-eslint/parser": "^2.31.0",
        "browserslist": "^4.12.0",
        "cross-env": "^7.0.2",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-typescript": "^7.2.1",
        "eslint-config-prettier": "^6.11.0",
        "eslint-formatter-pretty": "^3.0.1",
        "eslint-import-resolver-typescript": "^2.0.0",
        "eslint-plugin-eslint-comments": "^3.1.2",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jest": "^23.9.0",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-unicorn": "^19.0.1",
        "husky": "^4.2.5",
        "import-sort-cli": "^6.0.0",
        "import-sort-parser-babylon": "^6.0.0",
        "import-sort-parser-typescript": "^6.0.0",
        "import-sort-style-module": "^6.0.0",
        "jest": "^26.0.1",
        "lint-staged": "^10.2.2",
        "markdownlint-cli": "^0.23.0",
        "nodemon": "^2.0.3",
        "npm-run-all": "^4.1.5",
        "onchange": "^7.0.2",
        "prettier": "^2.0.5",
        "rimraf": "^3.0.2",
        "sort-package-json": "^1.42.2",
        "typescript": "^3.8.3"
    }
}
